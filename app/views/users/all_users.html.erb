<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Search count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.search_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back', users_path %>
