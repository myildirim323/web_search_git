<div class="container">
  <div class="row">
    <div class="col-md-6 col-centered">
      <h3>Registered Users</h3>
      <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th>Name</th>
            <th>Search Count</th>
          </tr>

        </thead>

        <tbody>
          <% @users.each do |user| %>
            <tr>
              <th scope="row"><%= user.id %></th>
              <td><%= user.name %></td>
              <td><%= user.search_count %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <div class="btn btn-dark">
        <%= link_to 'Back', users_path, :class => "btn"%>
      </div>
    </div>
  </div>
</div>
